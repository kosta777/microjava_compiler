program test301

	enum Broj { NULA, JEDAN, PET=5, DESET=10}

	const int jedan = 1;
	const int pet = 5;

	int niz[];
	char nizch[];
	
{
	void main()	
		int bodovi;
		Broj nula, jedan;
	{
		nula = Broj.NULA;
		jedan = Broj.JEDAN;	
		bodovi = 0;
		bodovi++;
		bodovi = bodovi + jedan;
		bodovi = bodovi * pet;
		bodovi--;
		print(bodovi); 

	}
}


