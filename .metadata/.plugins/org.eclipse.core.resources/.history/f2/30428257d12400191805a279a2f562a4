package rs.ac.bg.etf.pp1;

public class sym 
{
	// Keywords
	public static final int PROG = 1;
	public static final int BREAK = 2;
	public static final int CLASS = 3;
	public static final int INTERFACE = 4;
	public static final int ENUM = 5;
	public static final int ELSE = 6;
	public static final int CONST = 7;
	public static final int IF = 8;
	public static final int NEW = 9;
	public static final int PRINT = 10;
	public static final int READ = 11;
	public static final int VOID = 12;
	public static final int FOR = 13;
	public static final int EXTENDS = 14;
	public static final int CONTINUE = 15;
	public static final int RETURN = 16;
	
	// Identifiers
	public static final int IDENT = 17;
	public static final int LETTER = 18;
	public static final int DIGIT = 19;
	public static final int UNDERSCORE = 20;
	public static final int NUMCONST = 21;
	public static final int CHARCONST = 22;
	public static final int BOOLCONST = 23;
	public static final int BOOLTRUE = 24;
	public static final int BOOLFALSE = 25;
	// Constants
	
	// Operators
	public static final int PLUS = 6;
	public static final int EQUAL = 7;
	public static final int COMMA = 8;
	public static final int SEMI = 9;
	public static final int LPAREN = 10;
	public static final int RPAREN = 11;
	public static final int LBRACE = 12;
	public static final int RBRACE = 13;
	
	public static final int EOF = 14;
	
	
}
